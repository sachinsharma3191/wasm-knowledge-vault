{
  "name": "knowledge-vault",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && esbuild src/sw.ts --bundle --format=iife --outfile=dist/sw.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "wasm:build": "wasm-pack build wasm/kv_index --target web --out-dir ./src/wasm/kv_index --out-name kv_index",
    "wasm:clean": "rimraf ./src/wasm/kv_index && (cd wasm/kv_index && cargo clean)",
    "prefetch:model": "node scripts/prefetch-model.mjs"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "file-saver": "^2.0.5",
    "idb": "^8.0.3",
    "jszip": "^3.10.1",
    "minisearch": "^7.1.2",
    "pdfjs-dist": "^5.4.54",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/offscreencanvas": "^2019.7.3",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-plugin-top-level-await": "^1.6.0"
  }
}
